<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="YouthProtectionAplication.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:YouthProtectionAplication"
    xmlns:views="clr-namespace:YouthProtectionAplication.Views.Usuarios"
    Shell.FlyoutBehavior="Disabled"
    Title="YouthProtectionAplication">
    
    
    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate views:LoginView}"
        Route="MainPage" />

    <ShellContent
    Title="CreateAccount"
    ContentTemplate="{DataTemplate views:CreateAccountPage}"
    Route="CreateAccount" />

        
</Shell>
